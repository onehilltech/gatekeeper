    "core-object": "^3.1.5",
    "cors": "^2.8.4",
    "ejs": "^2.5.7",
    "email-templates": "^3.1.5",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "^8.1.0",
    "nodemailer": "^4.3.1",
    "nonce": "^1.0.4",
    "path": "^0.12.7",
    "prompt": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "request": "^2.83.0",
    "uid-safe": "^2.1.5",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^5.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemailer-mailgun-transport": "^1.3.5",
    "nodemailer-stub-transport": "^1.0.0",
    "superagent": "^3.8.0",
    "supertest": "^3.0.0"
